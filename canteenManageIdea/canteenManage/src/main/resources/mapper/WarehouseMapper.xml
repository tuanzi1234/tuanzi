<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.WarehouseMapper">

    <select id="selectAll" resultType="com.example.entity.Warehouse">
        select warehouse.* from `warehouse`
        <where>
            <if test="name != null"> and warehouse.name like concat('%', #{name}, '%')
            </if>
        </where>
    </select>


    <insert id="insert" parameterType="com.example.entity.Warehouse" useGeneratedKeys="true">
        insert into `warehouse`
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="name != null">name,</if>
            <if test="price != null">price,</if>
            <if test="num != null">num,</if>
            <if test="status != null">status,</if>
            <if test="time != null">time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="name != null">#{name},</if>
            <if test="price != null">#{price},</if>
            <if test="num != null">#{num},</if>
            <if test="status != null">#{status},</if>
            <if test="time != null">#{time},</if>
        </trim>
    </insert>

    <update id="updateById" parameterType="com.example.entity.Warehouse">
        update `warehouse`
        <set>
            <if test="name != null">
                name = #{name},
            </if>
            <if test="price != null">
                price = #{price},
            </if>
            <if test="num != null">
                num = #{num},
            </if>
            <if test="status != null">
                status = #{status},
            </if>
            <if test="time != null">
                time = #{time},
            </if>
        </set>
        where id = #{id}
    </update>

</mapper>